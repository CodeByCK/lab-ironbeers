<div class="container-fluid">
    <div class="row">
        {{#each beers}}
        <div class="col-md-4">
            <img class="card-img-top" src="{{this.image_url}}" alt="">
        </div>
        <div class="col-md-8">
            <h3 class="card-title">{{this.name}}</h3>
            <h6 class="card-subtittle">{{this.tagline}}</h6>
            <p class="card-text">{{this.description}}</p>
        </div>
        {{/each}}
    </div>
</div>